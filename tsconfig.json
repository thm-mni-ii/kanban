{
  "compilerOptions":
  {
    /* Language and Environment */
    "target": "es2016",
    "lib": ["es2016", "dom"],

    /* Modules */
    "module": "commonjs",

    /*Emit */
    "outDir": "dist",
    "sourceMap": true,

    /* Interop Constraints */
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,

    /* Type Checking */
    "strict": true,

    /* Completeness */
    "skipLibCheck": true
  },
  "files": [
    "groupService.ts"
  ]
}

